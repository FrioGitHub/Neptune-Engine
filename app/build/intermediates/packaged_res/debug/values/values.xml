<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="highlightColor">#85212121</color>
    <color name="ic_launcher_background">#000000</color>
    <color name="primaryColor">#121212</color>
    <color name="primaryVariantColor">#1D1D1D</color>
    <color name="strokeColor">#C5C6CF</color>
    <color name="white">#FFFFFF</color>
    <string name="app_name">Neptune Engine</string>
    <string name="cancel">Cancel</string>
    <string name="centralize">Center camera</string>
    <string name="create_project">Create project</string>
    <string name="delete">Delete</string>
    <string name="digits_valid_name">Please enter a valid name.</string>
    <string name="edit">Edit</string>
    <string name="name">Name</string>
    <string name="new_object">New object</string>
    <string name="no_projects">No projects</string>
    <string name="ok">Ok</string>
    <string name="project_already_exists">This project already exists.</string>
    <string name="projects">Projects</string>
    <string name="save">Save world</string>
    <string name="saved_successfully">World saved successfully!</string>
    <string name="square">Square</string>
    <string name="trash">Remove object</string>
    <style name="AppTheme" parent="Theme.Material3.DynamicColors.Dark">
    <item name="windowActionBar">false</item>
    <item name="windowNoTitle">true</item>
    <item name="android:statusBarColor">@android:color/transparent</item>
    <item name="android:textCursorDrawable">@drawable/cursor_drawable</item>
    <item name="android:windowBackground">@color/primaryColor</item>
    <item name="android:popupMenuStyle">@style/AppTheme.PopupMenu</item>
  </style>
    <style name="AppTheme.PopupMenu" parent="Widget.Material3.PopupMenu">
    <item name="android:popupBackground">@color/primaryVariantColor</item>
  </style>
    <style name="AppTheme.TextInputLayout" parent="">
    <item name="colorPrimary">@color/white</item>
    <item name="hintTextColor">@color/white</item>
  </style>
    <style name="AppTheme.Toolbar" parent="Widget.Material3.Toolbar">
    <item name="android:background">@color/primaryColor</item>
  </style>
    <style name="MaterialAlertDialog" parent="ThemeOverlay.Material3.MaterialAlertDialog">
    <item name="materialAlertDialogTitleTextStyle">@style/MaterialDialog.Title.Text</item>
    <item name="materialAlertDialogBodyTextStyle">@style/MaterialDialog.Body.Text</item>
    <item name="buttonBarPositiveButtonStyle">@style/MaterialAlertDialog.PositiveButtonStyle</item>
    <item name="buttonBarNegativeButtonStyle">@style/MaterialAlertDialog.NegativeButtonStyle</item>
    <item name="buttonBarNeutralButtonStyle">@style/MaterialAlertDialog.NeutralButtonStyle</item>
    <item name="android:background">@color/primaryColor</item>
  </style>
    <style name="MaterialAlertDialog.NegativeButtonStyle" parent="@style/Widget.Material3.Button.TextButton.Dialog">
    <item name="android:textColor">@color/white</item>
    <item name="backgroundTint">@android:color/transparent</item>
  </style>
    <style name="MaterialAlertDialog.NeutralButtonStyle" parent="@style/Widget.Material3.Button.TextButton.Dialog">
    <item name="android:textColor">@color/white</item>
    <item name="backgroundTint">@android:color/transparent</item>
  </style>
    <style name="MaterialAlertDialog.PositiveButtonStyle" parent="@style/Widget.Material3.Button.TextButton.Dialog">
    <item name="android:textColor">@color/white</item>
    <item name="backgroundTint">@android:color/transparent</item>
  </style>
    <style name="MaterialDialog.Body.Text" parent="@style/MaterialAlertDialog.Material3.Body.Text">
    <item name="android:textColor">@color/white</item>
  </style>
    <style name="MaterialDialog.Title.Text" parent="@style/MaterialAlertDialog.Material3.Title.Text">
    <item name="android:textColor">@color/white</item>
    <item name="android:textStyle">bold</item>
  </style>
</resources>